<?xml version='1.0' encoding='utf-8'?>
<interface version="1.0">
  <object class="tk.Toplevel" id="toplevel1">
    <property name="height">200</property>
    <property name="resizable">none</property>
    <property name="title" translatable="yes">Загрузка новых объявлений</property>
    <property name="width">200</property>
    <child>
      <object class="tk.Frame" id="mainwindow">
        <property name="height">200</property>
        <property name="padx">5</property>
        <property name="pady">5</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">nw</property>
        </layout>
        <child>
          <object class="tk.Label" id="start_name_file">
            <property name="font">{DejaVu Sans} 12 {}</property>
            <property name="text" translatable="yes">Начало имени файла</property>
            <layout manager="grid">
              <property name="propagate">True</property>
              <property name="sticky">nw</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Entry" id="ent_begin_file">
            <property name="font">{DejaVu Sans} 12 {}</property>
            <property name="width">50</property>
            <layout manager="grid">
              <property name="padx">5</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
              <property name="sticky">nw</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="lbl_cnt_pages">
            <property name="font">{DejaVu Sans} 12 {}</property>
            <property name="text" translatable="yes">Кол-во страниц, на которых будут искаться объявления:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">3</property>
              <property name="sticky">nw</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="lbl_list_url">
            <property name="font">{DejaVu Sans} 12 {}</property>
            <property name="text" translatable="yes">Список url-адресов для поиска объявлений</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">6</property>
              <property name="propagate">True</property>
              <property name="row">5</property>
              <property name="sticky">nw</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Text" id="txt_list_url">
            <property name="font">{DejaVu Sans} 10 {}</property>
            <property name="height">10</property>
            <property name="relief">sunken</property>
            <property name="width">63</property>
            <property name="wrap">word</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">6</property>
              <property name="sticky">nw</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="btn_start">
            <property name="text" translatable="yes">Получить новые объявления</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">6</property>
              <property name="propagate">True</property>
              <property name="row">7</property>
              <property name="sticky">nw</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Spinbox" id="spinbox_cnt_pages">
            <property name="from_">1</property>
            <property name="increment">1</property>
            <property name="to">1000</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">4</property>
              <property name="sticky">nw</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Checkbutton" id="chkBtn_excel_file">
            <property name="font">{DejaVu Sans} 12 {}</property>
            <property name="idtocommand">false</property>
            <property name="offvalue">False</property>
            <property name="onvalue">True</property>
            <property name="text" translatable="yes">Сохранять в Excel-файл</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
              <property name="sticky">nw</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
